if [ -z "$PACKAGE_VERSION" ]; then
    eval `grep PACKAGE_VERSION= configure`
    if [ "$BRANCH" = master ] && [ "$PACKAGE_VERSION" = 0.1 ]; then
       PACKAGE_VERSION=0.1.0
    fi
fi
CHANGES+=("hook: Bump version to $PACKAGE_VERSION")

patch -p1 < $HOOKS/wayland-demos.patch
CHANGES+=("hook: Update installation list.")
